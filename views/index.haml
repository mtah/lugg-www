%p= @page
%p= @req
- @feed_items.each do |item|
  - date = item[:date].strftime("%Y-%m-%d %H:%M CET") 
  .feed_item
    %h2
      %a{:href => item[:link]}= item[:title]
    %p.meta= "Publicerad #{date} av #{item[:user_name]}" 
    %p= strip_tags(item[:description], "img", "script")
    
